type OfferList {
  offerId: ID!
  accountId: String!
  offerType: String!
  offerTitle: String!
  offerSeverity: String!
  offerCatagory: String!
  offerLocation: String!
  offerStatus: String!
  offerText: String!
  createDate: String!
  updateDate: String!
}

type Query {
  OfferList(id: String!): OfferList
}

